	urldate = {2023-04-11},
	journal = {Science},
	author = {Armbrust, E. Virginia and Berges, John A. and Bowler, Chris and Green, Beverley R. and Martinez, Diego and Putnam, Nicholas H. and Zhou, Shiguo and Allen, Andrew E. and Apt, Kirk E. and Bechner, Michael and Brzezinski, Mark A. and Chaal, Balbir K. and Chiovitti, Anthony and Davis, Aubrey K. and Demarest, Mark S. and Detter, J. Chris and Glavina, Tijana and Goodstein, David and Hadi, Masood Z. and Hellsten, Uffe and Hildebrand, Mark and Jenkins, Bethany D. and Jurka, Jerzy and Kapitonov, Vladimir V. and Kröger, Nils and Lau, Winnie W. Y. and Lane, Todd W. and Larimer, Frank W. and Lippmeier, J. Casey and Lucas, Susan and Medina, Mónica and Montsant, Anton and Obornik, Miroslav and Parker, Micaela Schnitzler and Palenik, Brian and Pazour, Gregory J. and Richardson, Paul M. and Rynearson, Tatiana A. and Saito, Mak A. and Schwartz, David C. and Thamatrakoln, Kimberlee and Valentin, Klaus and Vardi, Assaf and Wilkerson, Frances P. and Rokhsar, Daniel S.},

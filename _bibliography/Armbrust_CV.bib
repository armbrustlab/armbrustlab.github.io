@article{campbell_identification_2001,
	title = {{IDENTIFICATION} {AND} {DNA} {SEQUENCE} {OF} {A} {NEW} {H}+-{ATPase} {IN} {THE} {UNICELLULAR} {GREEN} {ALGA} {CHLAMYDOMONAS} {REINHARDTII} ({CHLOROPHYCEAE})},
